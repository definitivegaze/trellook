{

    "manifest_version": 2,
    "name": "Trellook",
    "version": "0.0.1",
  
    "description": "Changing some visuals on trello.com",
    "homepage_url": "https://github.com/definitivegaze/trellook",
  
    "icons": {
      "48": "icons/colour-icon.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.trello.com/*"],
        "js": ["trellook.js"]
      }
    ]
  
  }
  